{
  "name": "morgan-mongo",
  "version": "0.0.1",
  "description": "Node.js HTTP request logger middleware for Express with MongoDB as storage; morgan and Mongoose based",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run lint && tsc",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "test": "./node_modules/.bin/mocha -r ts-node/register test/**/*.spec.ts",
    "postinstall": "npm run build",
    "start": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmstss/morgan-mongo.git"
  },
  "keywords": [
    "express",
    "mongo",
    "logger",
    "middleware",
    "morgan",
    "mongoose",
    "typescript"
  ],
  "author": "Viachaslau Tyshkavets",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pmstss/morgan-mongo/issues"
  },
  "homepage": "https://github.com/pmstss/morgan-mongo#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/mongoose": "^5.3.1",
    "@types/morgan": "^1.7.35",
    "@types/sinon": "^5.0.7",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "sinon": "^7.1.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "express": "^4.16.4",
    "mongoose": "^5.3.14",
    "morgan": "^1.9.1",
    "useragent": "^2.3.0"
  },
  "engines": {
    "node": "10.x"
  } 
}
